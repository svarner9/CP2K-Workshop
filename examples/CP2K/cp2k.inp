&FORCE_EVAL
   METHOD Quickstep
   &DFT
      BASIS_SET_FILE_NAME BASIS_PBE
      POTENTIAL_FILE_NAME PBE_POTENTIALS
      &QS
         METHOD GPW
         EXTRAPOLATION PS
         EXTRAPOLATION_ORDER 3
         EPS_DEFAULT 1.0E-10
      &END QS
      &POISSON
         PERIODIC XYZ
      &END POISSON
      &SCF
         EPS_SCF 1.0E-6
         SCF_GUESS ATOMIC
         MAX_SCF 50 # Use larger value for real simulations
         &OT T
            MINIMIZER DIIS
            PRECONDITIONER FULL_SINGLE_INVERSE
         &END OT
      &END SCF
      &XC
         &XC_FUNCTIONAL PBE
         &END XC_FUNCTIONAL
         &VDW_POTENTIAL
            DISPERSION_FUNCTIONAL PAIR_POTENTIAL
            &PAIR_POTENTIAL
               TYPE DFTD3
               PARAMETER_FILE_NAME dftd3.dat
               REFERENCE_FUNCTIONAL PBE
               R_CUTOFF [angstrom] 16.0 # Use larger cutoff for real simulations
            &END PAIR_POTENTIAL
         &END VDW_POTENTIAL
      &END XC
      &LS_SCF
         MAX_SCF 50
      &END LS_SCF
      &MGRID
         CUTOFF 300 # Use larger cutoff for real simulations
      &END MGRID
   &END DFT
   &SUBSYS
      &KIND H
         MASS 2.0
         BASIS_SET DZVP-MOLOPT-PBE-GTH-q1
         POTENTIAL GTH-PBE-q1
      &END KIND
      &KIND O
         BASIS_SET DZVP-MOLOPT-PBE-GTH-q6
         POTENTIAL GTH-PBE-q6
      &END KIND
      &KIND Na
         BASIS_SET DZVP-MOLOPT-PBE-GTH-q9
         POTENTIAL GTH-PBE-q9
      &END KIND
      &KIND Cl
